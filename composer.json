{
    "name": "adrien-mallet/secured-container-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SecuredContainerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SecuredContainerBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "amallet",
            "email": "adrien.mallet@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "symfony/dependency-injection": "^4.4",
        "symfony/config": "^4.4",
        "symfony/http-kernel": "^4.4"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.4"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "8"
    },
    "config": {
        "bin-dir": "bin"
    }
}
